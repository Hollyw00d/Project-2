                <a class="orange-btn expand" data-toggle="collapse" data-target="#groupRequests" href="#groupRequests"
                    role="button" aria-expanded="false" aria-controls="groupRequests">
                    Groups
                    <span class="arrowContainer">
                        <img class="expandArrow" src="/images/expandArrow.svg" alt="expand group requests">
                        <img class="collapseArrow" src="/images/collapseArrow.svg" alt="collapse group requests">
                    </span>
                </a>
            </div>




            <div class="sectionContents">
                <div id="groupRequests" class="collapse profileDisplay">
                    <div class="requestsDisplay">
                        <h4>Your Requests to Join Groups</h4>
                        <div class="requestContainer">
                            {{#if yourPendingGroups}}
                            <h5>Pending</h5>
                            {{#each yourPendingGroups}}
                            <div class="group">
                                <div class="groupDescription">
                                    <h6>{{this.groupName}}</h6>
                                    <p>{{this.groupDescription}}</p>
                                    <form>
                                        <button class="editRequest" data-requestid="{{this.groupId}}">Edit</button>
                                        <button class="deleteRequest" data-requestid="{{this.groupId}}">Delete</button>
                                    </form>
                                </div>
                            </div>
                            {{/each}}
                            {{else}}
                            <h5 class="text-center">You have no pending requests to join groups.</h5>
                            {{/if}}
                        </div>
                    </div>
                    <div class="requestsDisplay">
                        <h4>Requests to Join Your Groups</h4>
