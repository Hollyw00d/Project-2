                                        <button class="removeFromGroupRequest" data-requestid="{{this.groupId}}">Remove
                                            Member</button>
                                    </form>
                                </div>
                            </div>
                            {{/each}}
                            {{else}}
                            <h5 class="text-center">There are no confirmed members in your groups.</h5>
                            {{/if}}
                        </div>
                    </div>
                </div>
                <div id="itemRequests" class="collapse profileDisplay">
                    <div class="requestsDisplay">
                        <h4>Your Requests for Items</h4>
                        <div class="requestContainer">
                            {{#if yourPending}}
                            <h5>Pending</h5>
                            {{#each yourPending}}
                            <div class="item">
                                <div class="itemDescription">
                                    <h6>{{this.itemName}}</h6>
