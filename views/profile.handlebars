{{>header}}
<main>
    <h2>{{user.userName}}</h2>
    <p><img class="user-photo" src="{{user.userImage}}" alt="{{user.userName}} photo" /></p>
    <div class="container">
        <div class="section">
            <div class="sectionHeader">
                <h3>Requests</h3>
                <a class="orange-btn expand middle" data-toggle="collapse" data-target="#itemRequests"
                    href="#itemRequests" role="button" aria-expanded="false" aria-controls="itemRequests">
                    Items
                    <span class="arrowContainer">
                        <img class="expandArrow" src="/images/expandArrow.svg" alt="expand item requests">
